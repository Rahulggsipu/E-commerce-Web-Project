<!DOCTYPE html>
<html>
  <head>
    <title>Users List</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <div class="container">
      
      <h1>Users List</h1>
      <% if(users.length>0) { %>
      <table class="list-table">
        <thead>
          <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Username</th>
            <th>Email Id</th>
            <th>Contact</th>
          </tr>
        </thead>
        <tbody>

          <% for(var i=0; i<users.length;i++) { %>
            <tr>
              <td><%= users[i].first_name%></a></td>
              <td><%= users[i].last_name%></td>
              <td><%= users[i].user_name%></td>
              <td><%= users[i].email%></td>
              <td><%= users[i].contact_no%></td>
            </tr>
          <% } %>
        </tbody>
      </table>
      <% } else { %>
        <div>No Users found.</div>
      <% } %>
    </div>
  </body>
</html>
